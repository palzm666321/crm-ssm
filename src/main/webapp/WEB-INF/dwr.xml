<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
        "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
        "http://getahead.org/dwr/dwr20.dtd">

<dwr>
  <allow>
    <create creator="spring" javascript="clueServiceImpl">
      <param name="beanName" value="clueServiceImpl"/>
    </create>
    <convert converter="bean" match="com.bjpowernode.domain.Clue"/>

    <create creator="spring" javascript="clueRemarkServiceImpl">
      <param name="beanName" value="clueRemarkServiceImpl"/>
    </create>
    <convert converter="bean" match="com.bjpowernode.domain.ClueRemark"/>

    <create creator="spring" javascript="ClueActivityRelationServiceImpl">
      <param name="beanName" value="ClueActivityRelationServiceImpl"/>
    </create>
    <convert converter="bean" match="com.bjpowernode.domain.ClueActivityRelationVo"/>
    <convert converter="bean" match="com.bjpowernode.domain.ClueActivityRelation"/>

  </allow>
</dwr>